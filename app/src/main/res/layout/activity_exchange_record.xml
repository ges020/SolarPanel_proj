<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C4F5F6"
    tools:context=".ExchangeRecordActivity">

    <ImageView
        android:id="@+id/sun"
        android:layout_width="101dp"
        android:layout_height="128dp"
        android:layout_marginBottom="1020dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.827"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/collector"
        android:layout_width="428dp"
        android:layout_height="290.4dp"
        android:layout_marginEnd="188dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.237"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.080"
        app:srcCompat="@drawable/collector" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.27"
        android:background="#08bd19"/>

    <ImageView
    android:id="@+id/underground"
    android:layout_width="match_parent"
    android:layout_height="750dp"
    android:contentDescription="@string/app_name"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="1.02"
    app:srcCompat="@drawable/underground" />

    <ImageView
        android:id="@+id/menu_background"
        android:layout_width="200dp"
        android:layout_height="400dp"
        android:layout_marginStart="570dp"
        android:src="@drawable/menu_background"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.601" />

    <ImageView
        android:id="@+id/menu_frame1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="600dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42"
        app:srcCompat="@drawable/menu_frame" />

    <ImageView
        android:id="@+id/menu_pic1"
        android:layout_width="55dp"
        android:layout_height="75dp"
        android:layout_marginStart="610dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="@+id/underground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42"
        app:srcCompat="@drawable/inquiry" />

    <ImageView
        android:id="@+id/menu_pic4"
        android:layout_width="48dp"
        android:layout_height="66dp"
        android:layout_marginStart="620dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="@+id/underground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.71"
        app:srcCompat="@drawable/logout" />

    <ImageView
        android:id="@+id/menu_frame3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="600dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.614"
        app:srcCompat="@drawable/yellow" />

    <ImageView
        android:id="@+id/menu_pic3"
        android:layout_width="75dp"
        android:layout_height="57dp"
        android:layout_marginStart="600dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="@+id/underground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55"
        app:srcCompat="@drawable/price" />

    <ImageView
        android:id="@+id/menu_frame4"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="600dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719"
        app:srcCompat="@drawable/menu_frame" />

    <ImageView
        android:id="@+id/list_background"
        android:layout_width="420dp"
        android:layout_height="588dp"
        android:layout_marginEnd="152dp"
        android:contentDescription="@string/app_name"
        android:src="@drawable/list_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.825" />

    <ListView
        android:id="@+id/listview"
        android:layout_width="390dp"
        android:layout_height="539dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        app:layout_constraintVertical_bias="0.88"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background" />

</androidx.constraintlayout.widget.ConstraintLayout>